#!/bin/bash
# Change dir to cwd
#$ -cwd
#$ -e ./map_to_intergenic/output
#$ -o ./map_to_intergenic/output
#$ -N map_to_intergenic
# Enter an array job
#$ -t 1-9
bwa aln -t 24 ~/workspace/arabidopsis_refseqs/ig_tair10.fasta deg_${SGE_TASK_ID}.fasta > ./map_to_intergenic/deg_${SGE_TASK_ID}.sai
bwa samse ~/workspace/arabidopsis_refseqs/ig_tair10.fasta ./map_to_intergenic/deg_${SGE_TASK_ID}.sai deg_${SGE_TASK_ID}.fasta > ./map_to_intergenic/deg_${SGE_TASK_ID}.sam

